# test package
pkgname="subpkg-dep-leak"
pkgver="1.0"
pkgrel=0
pkgdesc="Dummy test package with subpackages and dependencies"
url="https://gitlab.alpinelinux.org/alpine/aports"
arch="noarch"
depends="tar scanelf"
license="MIT"
subpackages="$pkgname-subpkg"

prepare() {
	mkdir -p "$builddir"
}

check() {
	true
}

package() {
	mkdir -p "$pkgdir"
}

subpkg() {
	depends="sed"
	mkdir -p "$subpkgdir"
}
